<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.example.wsr.Profile">

    <Button
        android:id="@+id/button32"
        android:layout_width="333dp"
        android:layout_height="57dp"
        android:backgroundTint="@color/cardview_light_background"
        android:text="Edit Profile"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        app:icon="@drawable/podd"
        app:iconGravity="start"
        app:iconPadding="0dp"
        app:iconSize="30dp"
        app:iconTint="#000000"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="228dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profile"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="39dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="333dp"
        android:layout_height="57dp"
        android:backgroundTint="@color/cardview_light_background"
        android:text="Edit Profile"
        android:textAlignment="viewStart"
        android:textColor="#000000"
        app:icon="@drawable/podd"
        app:iconGravity="start"
        app:iconPadding="0dp"
        app:iconSize="30dp"
        app:iconTint="#000000"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="155dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:width="60dp"
        android:height="60dp"
        android:clipToOutline="true"
        android:pathData="M30 0C46.5685 0 60 13.4315 60 30C60 46.5685 46.5685 60 30 60C13.4315 60 0 46.5685 0 30C0 13.4315 13.4315 0 30 0Z"
        android:strokeWidth="2"
        android:strokeColor="#CFCFCF"
        android:viewportWidth="60"
        android:viewportHeight="60"
        app:circularflow_defaultRadius="60dp"
        app:circularflow_radiusInDP="60"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="82dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Hello Ken"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        tools:layout_editor_absoluteY="90dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="Current balance:"
        app:layout_constraintEnd_toStartOf="@+id/textView19"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        tools:layout_editor_absoluteY="109dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="79dp"
        android:layout_height="18dp"
        android:gravity="top"
        android:lineSpacingExtra="2sp"
        android:text="N10,712:00"
        android:textColor="#0560FA"
        android:textSize="16sp"
        android:translationY="-0.97sp"
        tools:layout_editor_absoluteX="219dp"
        tools:layout_editor_absoluteY="109dp" />

</androidx.constraintlayout.widget.ConstraintLayout>